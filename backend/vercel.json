{
  "version": 2,
  "builds": [{ "src": "*.ts", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://ai-chatbot-front-end.vercel.app",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}
